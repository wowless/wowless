{
    "build": {
        "context": "..",
        "dockerfile": "Dockerfile"
    },
    "customizations": {
      "codespaces": {
        "repositories": {
          "lua-wow-tools/*": {
            "permissions": "write-all"
          }
        }
      },
      "vscode": {
        "extensions": [
          "EditorConfig.EditorConfig"
        ]
      }
    },
    "features": {
        "github-cli": {}
    },
    "name": "wowless/wowless",
    "postCreateCommand": "pre-commit install",
    "remoteUser": "vscode"
}
